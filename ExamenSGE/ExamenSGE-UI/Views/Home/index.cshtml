@model IEnumerable<ExamenSGE_Entities.clsMision>

@{
    ViewBag.Title = "index";
}

<h2>index</h2>

<form method="post">

    <div class="form-group">
        <label>Misiones:</label>
        <div class="col-md-10">
            <select name="id">
                @foreach (var item in Model)
                {
                    <option value="@item.Id">
                        @item.Nombre
                    </option>
                }
            </select>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" name="Enviar" value="Enviar" />
        </div>
    </div>

</form>




</table>
